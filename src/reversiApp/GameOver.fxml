<HBox fx:id="root" xmlns="http://javafx.com/javafx"
      xmlns:fx="http://javafx.com/fxml"
      fx:controller="reversiApp.GameOver">
    <VBox spacing="10">
        <Label alignment="TOP_LEFT" contentDisplay="TOP" prefHeight="59.0" prefWidth="150.0" text="Game Over:" GridPane.columnIndex="1" GridPane.rowIndex="1" />
        <Label fx:id="winner" alignment="TOP_LEFT" contentDisplay="TOP" prefHeight="59.0" prefWidth="150.0" text="winner:" GridPane.columnIndex="1" GridPane.rowIndex="1" />
        <Label fx:id="XScore" alignment="TOP_LEFT" contentDisplay="TOP" prefHeight="59.0" prefWidth="150.0" text="Score X:" GridPane.columnIndex="1" GridPane.rowIndex="1" />
        <Label fx:id="OScore" alignment="TOP_LEFT" contentDisplay="TOP" prefHeight="59.0" prefWidth="150.0" text="Score O:" GridPane.columnIndex="1" GridPane.rowIndex="1" />


        <Button mnemonicParsing="false" onAction="#returnToMenu" text="return to the menu" />
    </VBox>

</HBox>