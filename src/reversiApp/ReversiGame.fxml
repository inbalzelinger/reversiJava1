<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.shape.*?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.shape.Circle?>

<HBox fx:id="root" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="reversiApp.ReversiGameController">
   <children>
      <GridPane prefHeight="641.0" prefWidth="212.0">
        <columnConstraints>
            <ColumnConstraints hgrow="SOMETIMES" maxWidth="92.0" minWidth="10.0" prefWidth="37.0" />
          <ColumnConstraints hgrow="SOMETIMES" maxWidth="147.0" minWidth="10.0" prefWidth="147.0" />
        </columnConstraints>
        <rowConstraints>
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
        </rowConstraints>
         <children>
            <Circle fx:id="playerCircle" fill="DODGERBLUE" radius="12.0" stroke="BLACK" strokeType="INSIDE" />
            <Circle fx:id="playerCircle1" fill="DODGERBLUE" radius="12.0" stroke="BLACK" strokeType="INSIDE" GridPane.rowIndex="1" />
            <Circle fx:id="playerCircle2" fill="DODGERBLUE" radius="12.0" stroke="BLACK" strokeType="INSIDE" GridPane.rowIndex="2" />
      
              <Label fx:id="labelVar" alignment="TOP_LEFT" contentDisplay="TOP" prefHeight="23.0" prefWidth="99.0" text="current Player:" GridPane.columnIndex="1" />
      
              <Label fx:id="XScore" alignment="TOP_LEFT" contentDisplay="TOP" prefHeight="26.0" prefWidth="147.0" text="Score X:" GridPane.columnIndex="1" GridPane.rowIndex="1" />
              <Label fx:id="OScore" alignment="TOP_LEFT" contentDisplay="TOP" prefHeight="25.0" prefWidth="147.0" text="Score O:" GridPane.columnIndex="1" GridPane.rowIndex="2" />
      
      
              <Button mnemonicParsing="false" onAction="#end" text="End Game" GridPane.rowIndex="3" />
         </children>
      </GridPane>
   </children>

</HBox>
